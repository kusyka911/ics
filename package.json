{
  "name": "@kusyka911/ics",
  "version": "3.7.1-build.7",
  "description": "iCal (ics) file generator",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "start": "mocha --require @babel/register --watch --recursive",
    "test": "mocha --require @babel/register --recursive",
    "build": "rm -r dist; gulp build",
    "vite": "vite",
    "vite:build": "vite build",
    "vite:dist": "vite build; vite build --mode=bundled"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adamgibbons/ics.git"
  },
  "keywords": [
    "ical",
    "ics",
    "calendar",
    "icalendar",
    "generator",
    "date",
    "date-time",
    "events",
    "alarms"
  ],
  "author": "Adam Gibbons <adam@agibbons.com> (http://agibbons.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adamgibbons/ics/issues"
  },
  "homepage": "https://github.com/adamgibbons/ics",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/register": "^7.6.2",
    "chai": "^4.2.0",
    "dayjs": "^1.8.33",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "mocha": "^9.1.3",
    "vite": "^5.0.10",
    "yargs-parser": ">=13.1.2"
  },
  "dependencies": {
    "nanoid": "^3.1.23",
    "runes2": "^1.1.2",
    "yup": "^1.2.0"
  },
  "files": [
    "dist/",
    "index.d.ts"
  ]
}
